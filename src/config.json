{
    "app" : {
        "baseDir" : "app",
        "distFolder" : "dist/*",
        "js" : {
            "src" : "app/js/**/*.js",
            "srcBabel" : "app/js/*.js",
            "dest" : "app/js/transpiled"
        },
        "assets" : {
            "src" : "app/assets/**/*",
            "dest" : "dist/assets/"
        },
        "html" : {
            "index" : "./app/index.html",
            "src" : "app/index.html"
        },
        "sass" : {
            "src" : "app/scss/**/*.scss",
            "dest" : "app/css"
        },
        "css" : {
            "src" : "./app/css/*",
            "dest" : "app/css/"
        }
    }
}
